{
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html",
                "/*.css",
                "/*.js"
            ],
            "urls": [
                "https://fonts.googleapis.com/**"
            ]

        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**"
            ]
        }
    }],
    "dataGroups": [{
        "name": "number-api",
        "urls": ["http://numbersapi.com/**"],
        "cacheConfig": {
            "stratergy": "preformance",
            "maxSize": 5,
            "maxAge": "1h",
            "timeout": "5s"
        }
    }]
}